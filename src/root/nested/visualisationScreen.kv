<DataSelectScreen>:
	name: "selection"
	canvas.before:
        Color:
            rgba: 1, 1, 1, 1
        Rectangle:
            pos: self.pos
            size: self.size
	Button:
		text: "Data Selection Screen"
		pos: 50, 50
		size_hint: (0.5, 0.5)

<DataViewScreen>:
	name: "display"
	Label:
		text: "Data Selection Screen"

<VisualisationScreen>:
	name: "screen1"
	ScreenManager:
		DataSelectScreen
		DataViewScreen